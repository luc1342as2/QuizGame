<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="PromptScreenFXMLController">
   <children>
      <TitledPane animated="false" prefHeight="400.0" prefWidth="642.0" style="-fx-border-color: red;" text="Prompt Message">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="254.0" prefWidth="1225.0" style="-fx-background-color: black;">
               <children>
                  <Label fx:id="lbltxt" layoutX="47.0" layoutY="39.0" prefHeight="118.0" prefWidth="549.0" text="Label" textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="18.0" />
                     </font>
                  </Label>
                  <Button fx:id="yesbtn" layoutX="104.0" layoutY="206.0" mnemonicParsing="false" onAction="#yesbtn_M" style="-fx-background-color: red;" text="Yes" textFill="#eaeaea">
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font>
                  </Button>
                  <Button fx:id="no_Btn" layoutX="476.0" layoutY="206.0" mnemonicParsing="false" onAction="#noBtn_M" style="-fx-background-color: red;" text="No" textFill="#eaeaea">
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font>
                  </Button>
               </children>
            </AnchorPane>
        </content>
      </TitledPane>
   </children>
</AnchorPane>
